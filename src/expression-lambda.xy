let expression_lambda (bodies) =	with (func = { name = "", bodies = bodies })		(@(o, compiler) =			with ([func, compiler] = compiler_push_lambda(compiler, func))				["\txy_value_set_function(" + o + ", " + func::qname + ", c);\n",				compiler])		-> expression_no_constlet expression_binary_lambda (op) =	(@(o, compiler) = 		["\txy_value_set_function(" + o + ", " + binary_op_string(op) + "_lambda, NULL);\n",		compiler])	-> expression_no_const